{
  "name": "polkaswitch-ui",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "description": "",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run patch-google-proto && npm run clean && npm run build-app && npm run build-claim && npm run build-metrics",
    "build-app": "webpack --mode production --env production",
    "build-claim": "webpack --mode production --env production --env claim",
    "build-metrics": "webpack --mode production --env production --env metrics",
    "ci-build": "webpack --mode production --env development",
    "watch": "webpack --watch --mode development --env development",
    "profile": "webpack --profile --json=stats.$(date +'%Y_%m_%d_%I_%M_%p').json",
    "start": "node src/server/index.js",
    "server": "nodemon -r dotenv/config src/server/index.js",
    "format": "prettier --write \"src/**/*.js*\"",
    "lint": "eslint \"src/**/*.js*\" --fix",
    "test": "NODE_OPTIONS=--unhandled-rejections=warn jest --forceExit",
    "patch-google-proto": "replace goog.DEBUG=!0 goog.DEBUG=!1 node_modules/google-protobuf/google-protobuf.js"
  },
  "author": "polkaswitch",
